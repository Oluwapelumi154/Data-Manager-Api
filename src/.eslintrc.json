{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 10
  },
  "plugins": ["react"],
  "rules": {
    "space-in-parens": "warn",
    "semiQuotes": ["double"]
  }
}
